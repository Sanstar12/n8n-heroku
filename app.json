{
  "name": "n8n",
  "description": "Deploy n8n to Heroku using Docker",
  "stack": "container",
  "env": {
    "GENERIC_TIMEZONE": {
      "value": "Asia/Kolkata"
    },
    "N8N_ENCRYPTION_KEY": {
      "value": "n8n_secure_key_do_not_change_123456789"
    },
    "WEBHOOK_URL": {
      "value": "https://<appname>.herokuapp.com/"
    },
    "DB_POSTGRESDB_SSL_REJECT_UNAUTHORIZED": {
      "value": "false"
    }
  }
}
